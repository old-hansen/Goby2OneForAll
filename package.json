{
    "name": "OneForAll",
    "publisher": "Hansen",
    "description": "Goby 2 OneForAll , OneForAll is a powerful subdomain integration tool ， 结合了DNS记录查询和MessDNS爆破的高效域名收集工具",
    "version": "0.1.0",
    "icon": "./src/assets/icon/DNS.svg",
    "engines": "1.8.228",
    "initEvents": "",
    "main": "./src/extension.js",
    "contributes": {
        "configuration": {
            "oneforall.py" :{
                "type": "string",
                "default": null,
                "description": "请输入 oneforall.py 的路径",
                "fromDialog": true
            },
            "python3": {
                "type": "string",
                "default": "python3",
                "description": "请输入在当前环境下的Python3或位置,也可以是绝对路径",
                "fromDialog": true
            }
        },
        "views": {
            "toolbar": [
                {
                    "command": "showView",
                    "title": "OneForAll",
                    "icon": "./src/assets/icon/DNS.svg",
                    "tips":"OneForAll"
                }
            ]
        }
    },
    "scripts": {},
    "devDependencies": {},
    "dependencies": {}
}
